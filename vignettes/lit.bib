 number = {15-16},
 issn = {1436-5073},
 journal = {{Microchimica Acta}},
 doi = {\url{10.1007/s00604-014-1242-5}},
 file = {febbfc10-a7d3-43bd-b0a7-659a05f8c250:C\:\\Users\\Weronika Schary\\AppData\\Local\\Swiss Academic Software\\Citavi 6\\ProjectCache\\j8aglyuzhkrxdpvs4fm5han730sefzmcrez7exbz\\Citavi Attachments\\febbfc10-a7d3-43bd-b0a7-659a05f8c250.pdf:pdf}
