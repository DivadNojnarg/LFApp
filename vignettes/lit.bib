 keywords = {Biomedicine;DNA, Complementary/metabolism;general;Image Processing, Computer-Assisted/methods;Medical Biochemistry;Molecular Medicine;Oligonucleotide Array Sequence Analysis/methods;RNA, Messenger/metabolism;Software;Stem Cells},
 pages = {83--88},
