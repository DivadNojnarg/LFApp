@article{Carrell.2019,
 abstract = {The interest in and use of microfluidic paper-based analytical devices (\textgreek{m}PADs) has grown exponentially over the last decade. Cellulose, and modified cellulose, has been used for centuries for making chemical measurements but devices made with this material traditionally suffered from either poor detection limits and/or limited ability to provide quantitative measurements. \textgreek{m}PADs address these problems by patterning paper to create microfluidic channel networks that can direct flow to different regions of the device without the need for external pumps used in most traditional microfluidic devices. Furthermore, because the devices are made from cellulose or modified cellulose using inexpensive manufacturing methods, the devices can be inexpensive and disposable. The ability to carry out multiplexed analysis without external pumps using inexpensive, disposable devices makes \textgreek{m}PADs ideal for point-of-care (POC) analysis. The interest in \textgreek{m}PADs has led to a number of excellent comprehensive reviews in the field; in this review, we do not attempt to reference the entire field but instead seek to highlight major developments as well as areas that will be important for future development of this field.},
 author = {Carrell, Cody and Kava, Alyssa and Nguyen, Michael and Menger, Ruth and Munshi, Zarina and Call, Zachary and Nussbaum, Mark and Henry, Charles},
 year = {2019},
 title = {{Beyond the lateral flow assay: A review of paper-based microfluidics}},
 url = {\url{http://www.sciencedirect.com/science/article/pii/S0167931718305069}},
 pages = {45--54},
 volume = {206},
 issn = {0167-9317},
 journal = {{Microelectronic Engineering}},
